{
  "exclude": ["node_modules", "private", ".vscode", ".expo"],
  "checkJs": false,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@data/*": ["./data/*"],
      "@assets/*": ["./assets/*"],
      "@screens/*": ["./src/screens/*"],
      "@utils/*": ["./src/utils/*"],
      "@store": ["./src/store"],
      "@store/*": ["./src/store/*"],
      "@navigation/*": ["./src/navigation/*"]
    }
  }
}
